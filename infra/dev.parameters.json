{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "projectName": {
      "value": "${AZURE_PROJECT_NAME}"
    },
    "environmentName": {
      "value": "${AZURE_ENVIRONMENT_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "openAILocation": {
      "value": "${AZURE_OPENAI_LOCATION}"
    },
    "aspCoreEnvironment": {
      "value": "Development"
    },
    "semanticSearchSku": {
      "value": "Standard"
    }
  }
}